{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DATABASE_CONNECTION_STRING_HERE"
  },
  "BlobStorage": {
    "ConnectionString": "YOUR_AZURE_STORAGE_CONNECTION_STRING_HERE",
    "ContainerName": "flyers"
  },
  "OmniWhatsApp": {
    "BaseUrl": "https://alots.io/v20.0",
    "PhoneNumberId": "YOUR_OMNI_PHONE_NUMBER_ID",
    "ApiKey": "YOUR_OMNI_API_KEY",
    "LanguageCode": "en",
    "Day0TemplateName": "review_request_day0",
    "Day1TemplateName": "review_reminder_day1",
    "Day3TemplateName": "review_reminder_day3"
  },
  "ReviewSchedule": {
    "Message2DelayMinutes": 1440,
    "Message3DelayMinutes": 4320,
    "PollingIntervalSeconds": 3600
  }
}
